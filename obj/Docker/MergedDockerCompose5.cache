name: dockercompose10111193774335795178
services:
  inflowdb:
    container_name: discountdb
    environment:
      POSTGRES_DB: inflowdb
      POSTGRES_PASSWORD: admin1234
      POSTGRES_USER: admin
    image: postgres
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5432
      published: "5432"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: postgres_data
      target: /var/lib/postgresql/data
      volume: {}
  inflowsystem.bootstrapper:
    build:
      context: C:\Users\Somad\source\repos\InflowSystem
      dockerfile: Bootstrapper/InflowSystem.Bootstrapper/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: inflowsystembootstrapper
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
networks:
  default:
    name: dockercompose10111193774335795178_default
volumes:
  postgres_data:
    name: dockercompose10111193774335795178_postgres_data